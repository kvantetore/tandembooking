  <ItemGroup>
    <None Include="PageContent\bergen-no.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Include="PageContent\bergen-en.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
