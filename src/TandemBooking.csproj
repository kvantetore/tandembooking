<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UserSecretsId>TandemBooking</UserSecretsId>
    <AssemblyName>TandemBooking</AssemblyName>
